type PixKey {
    id: ID!
    pixKeyType: String!
    pixKey: String!
    account: Account!
}

type Account {
    accountType: String!
    number: Int!
    agency: Int!
    holder: Holder!
}

type Holder {
    name: String!
    lastName: String
}

input NewPixKey {
    pixKeyType: String!
    pixKey: String!
    accountType: String!
    accountNumber: Int!
    agencyNumber: Int!
    holderName: String!
    holderLastName: String
}

type Query {
    pixKeys: [PixKey!]!
    pixKey(id: ID!): PixKey
    account(id: ID!): Account
    holder(id: ID!): Holder
}

type Mutation {
    createPixKey(newPixKey: NewPixKey!): PixKey!
    updatePixKey(id: ID!, newPixKey: NewPixKey!): PixKey!
    deletePixKey(id: ID!): PixKey!
}